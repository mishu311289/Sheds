#import "motivational.h"

@implementation motivational
@synthesize motivation_id,motivation,author;
-(id) initWithMotivationID:(NSString *)motivation_id motivation:(NSString *)motivation author:(NSString *)author
{
    if ((self = [super init])) {
        self.motivation_id = motivation_id;
        self.motivation = motivation;
        self.author  = author;
    }
    return self;

}
@end
